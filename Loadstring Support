-- Scripts:

local function FJJBYOW_fake_script() -- skyromain.name and avatar finder 
	local script = Instance.new('LocalScript', skyromain)

	local skyromain = script.parent
	
	local player = game.Players.LocalPlayer
	
	local userid = player.UserId
	local thumType = Enum.ThumbnailType.AvatarBust
	local thumSize = Enum.ThumbnailSize.Size420x420
	local content = game.Players:GetUserThumbnailAsync(userid, thumType, thumSize)
	
	skyromain.PlayerImage.Image = content
	skyromain.PlayerName.Text = player.Name
end
coroutine.wrap(FJJBYOW_fake_script)()
local function YYIOYO_fake_script() -- changekey.LocalScript 
	local script = Instance.new('LocalScript', changekey)

	local player = game.Players.LocalPlayer
	script.Parent.MouseButton1Click:Connect(function()
		wait(0.5)
		player.PlayerGui.skryo:Destroy()
		loadstring(game:HttpGet('https://raw.githubusercontent.com/ShibaGT/MyRobloxScripts/main/skyro'))()
	end)
end
coroutine.wrap(YYIOYO_fake_script)()
local function CBJA_fake_script() -- universal.changepage 
	local script = Instance.new('LocalScript', universal)

	script.Parent.MouseButton1Click:Connect(function() --universal
		game.Players.LocalPlayer.PlayerGui.skryo.skyromain.changekey.Visible = false
		game.Players.LocalPlayer.PlayerGui.skryo.skyromain.PlayerName.Visible = false
		game.Players.LocalPlayer.PlayerGui.skryo.skyromain.PlayerImage.Visible = false
		game.Players.LocalPlayer.PlayerGui.skryo.skyromain.creditstext.Visible = false
		game.Players.LocalPlayer.PlayerGui.skryo.skyromain.Rejoin.Visible = true
		game.Players.LocalPlayer.PlayerGui.skryo.skyromain.respawn.Visible = true
		game.Players.LocalPlayer.PlayerGui.skryo.skyromain.hamster.Visible = true
		game.Players.LocalPlayer.PlayerGui.skryo.skyromain.fly.Visible = true
		game.Players.LocalPlayer.PlayerGui.skryo.skyromain["ctrl click tp"].Visible = true
		game.Players.LocalPlayer.PlayerGui.skryo.skyromain["walk on walls"].Visible = true
		game.Players.LocalPlayer.PlayerGui.skryo.skyromain.noclip.Visible = true
		game.Players.LocalPlayer.PlayerGui.skryo.skyromain.btools.Visible = true
		game.Players.LocalPlayer.PlayerGui.skryo.skyromain.Telekinesis.Visible = true
		game.Players.LocalPlayer.PlayerGui.skryo.skyromain["Inf Jump"].Visible = true
		game.Players.LocalPlayer.PlayerGui.skryo.skyromain.AntiFling.Visible = true
		game.Players.LocalPlayer.PlayerGui.skryo.skyromain["CMD-X"].Visible = false
		game.Players.LocalPlayer.PlayerGui.skryo.skyromain["Inf Yield"].Visible = false
		game.Players.LocalPlayer.PlayerGui.skryo.skyromain["R15 Invisiblity"].Visible = true
		game.Players.LocalPlayer.PlayerGui.skryo.skyromain.Spiderman.Visible = true
	end)
end
coroutine.wrap(CBJA_fake_script)()
